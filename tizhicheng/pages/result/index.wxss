page {
  background-color: #f2f2f2;
}

.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}

.log-item {
  margin: 10rpx;
}

.green-back {
  height: 730rpx;
  background-image: linear-gradient(0deg, rgba(255, 255, 255, 0.00) 0%, #0acea9 15%, #00cfc4 100%);
  width: 100%;
}

.score {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 25rpx;
}

.weight {
  width: 264rpx;
  height: 264rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.35);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.circle {
  width: 242rpx;
  height: 242rpx;
  background-color: #34d7c7;
  border-radius: 50%;
  text-align: center;
  line-height: 235rpx;
}

.circle-num {
  font-size: 60rpx;
  color: #fff;
  display: inline-block;
}

.part {
  display: inline-block;
  font-size: 20rpx;
  color: #fff;
}

.percent {
  position: relative;
  border-radius: 8rpx;
  background: #fff;
  margin: 29rpx 20rpx 20rpx 20rpx;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  margin-top: -380rpx;
}

.percent-word {
  text-align: center;
  font-size: 30rpx;
  color: #4a4a4a;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #e6e6e6;
}

.percent-bottom {
  padding-left: 10rpx;
  padding-right: 10rpx;
  display: flex;
  flex-direction: column;
}

.percent-img {
  width: 70rpx;
  height: 70rpx;
}

.percent-desc {
  font-size: 24rpx;
  color: #595959;
  margin-top: 10rpx;
}

.one-part {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-top: 50rpx;
  padding-bottom: 50rpx;
}

.top-part {
  padding-top: 20rpx;
}

.bottom-part {
  padding-bottom: 20rpx;
}

.percent-row {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  flex: 1;
}

.percent-row:nth-child(1) {
  margin-top: 30rpx;
}

.one-part:nth-child(2) {
  border-right: 1px solid #e6e6e6;
  border-left: 1px solid #e6e6e6;
}

.percent-row:nth-child(2) {
  border-bottom: 1px solid #e6e6e6;
  border-top: 1px solid #e6e6e6;
}
