/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}

button {
  padding-left: 0px;
  padding-right: 0px;
  border-radius: 50rpx;
  margin-left: 0px;
  margin-right: 0px;
  border: unset;
  box-sizing: unset;
  text-align: unset;
  display: flex;
  flex-direction: column;
  line-height: unset;
  background: unset;
  background-color: unset;
}

button::after {
  border: unset;
}

page {
  background-color: rgb(242, 242, 242);
  width: 100%;
  height: 100%;
  position: absolute;
}

text {
  color: black;
  font-size: 30rpx;
}

.bg {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  display:inline-block;
}

.question_img {
  height: 405rpx;
  width: 542rpx;
  border: 4rpx solid black;
  margin-top: 50rpx;
}

.frameLayout {
  position: relative;
  align-items: center;
  flex-direction: column;
  display: flex;
}

.frameChild {
  position:absolute;
}